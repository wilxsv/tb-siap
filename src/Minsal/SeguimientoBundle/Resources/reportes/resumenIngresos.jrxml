	<parameter name="id_servicio" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_inicio" class="java.lang.String">
		<defaultValueExpression><![CDATA[28-03-2014]]></defaultValueExpression>
	</parameter>
	<parameter name="fecha_fin" class="java.lang.String">
		<defaultValueExpression><![CDATA[24-03-2014]]></defaultValueExpression>
	</parameter>
	<parameter name="bandera" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{id_servicio} !=0 ? "AND E.id_ambiente_ingreso="+$P{id_servicio}:""]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT initcap(
A.primer_apellido||coalesce(' '||A.segundo_apellido,'')||coalesce(' '||A.apellido_casada,'')||', '||
A.primer_nombre||coalesce(' '||A.segundo_nombre,'')||coalesce(' '||A.tercer_nombre,'')) as nombre_paciente,
E.id id_ingreso,
B.numero,
D.nombre_ambiente ambiente,
E.diagnostico,
to_char(E.fecha,'DD-MM-YYYY')||' '||to_char(E.hora,'HH24:MI') as fecha_ingreso,
F.nombre establecimiento,
to_char(A.fecha_nacimiento,'DD-MM-YYYY') as fecha_nacimiento
FROM mnt_paciente A
     INNER JOIN mnt_expediente B ON B.id_paciente=A.id
     INNER JOIN sec_ingreso E ON E.id_expediente=B.id
     LEFT JOIN mnt_aten_area_mod_estab D ON E.id_ambiente_ingreso=D.id
     INNER JOIN ctl_establecimiento F ON F.id=B.id_establecimiento
WHERE  B.habilitado= TRUE AND E.fecha>=to_date($P{fecha_inicio},'DD-MM-YYYY')
AND E.fecha<=to_date($P{fecha_fin},'DD-MM-YYYY') $P!{bandera}
ORDER BY A.primer_Apellido ASC, E.fecha DESC]]>
	</queryString>
