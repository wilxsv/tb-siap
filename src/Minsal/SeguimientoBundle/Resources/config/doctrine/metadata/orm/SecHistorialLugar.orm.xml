<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SecHistorialLugar" table="sec_historial_lugar">
    <indexes>
      <index name="IDX_8AAD6DC31827296" columns="id_historial_clinico"/>
      <index name="IDX_8AAD6DCD166B38A" columns="id_tipo_lugar_trabajo"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="text" column="nombre" nullable="true"/>
    <many-to-one field="idTipoLugarTrabajo" target-entity="CtlTipoLugarTrabajo">
      <join-columns>
        <join-column name="id_tipo_lugar_trabajo" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idHistorialClinico" target-entity="SecHistorialClinico">
      <join-columns>
        <join-column name="id_historial_clinico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
