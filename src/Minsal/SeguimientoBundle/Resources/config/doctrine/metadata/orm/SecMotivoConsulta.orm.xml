<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SecMotivoConsulta" table="sec_motivo_consulta">
    <indexes>
      <index name="fki_historial_clinico_motivo_consulta" columns="id_historial_clinico"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="consultaPor" type="text" column="consulta_por" nullable="false"/>
    <field name="presentaEnfermedad" type="text" column="presenta_enfermedad" nullable="false"/>
    <many-to-one field="idHistorialClinico" target-entity="SecHistorialClinico">
      <join-columns>
        <join-column name="id_historial_clinico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
