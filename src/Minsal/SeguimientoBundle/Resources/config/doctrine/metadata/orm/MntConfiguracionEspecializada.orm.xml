<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MntConfiguracionEspecializada" table="mnt_configuracion_especializada">
    <indexes>
      <index name="IDX_6897E7F02294B556" columns="id_especializada"/>
      <index name="IDX_6897E7F07DFA12F6" columns="id_establecimiento"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fechaInicio" type="date" column="fecha_inicio" nullable="true"/>
    <field name="fechaFin" type="date" column="fecha_fin" nullable="true"/>
    <field name="recetaDelDia" type="integer" column="receta_del_dia" nullable="true"/>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEspecializada" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_especializada" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
