<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CtlOtroAntecedente" table="ctl_otro_antecedente">
    <indexes>
      <index name="fki_tipo_antecedente_otro_antecedente" columns="id_tipo_antecedente"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="50" nullable="false"/>
    <field name="nombreAlternativo" type="string" column="nombre_alternativo" length="100" nullable="true"/>
    <many-to-one field="idTipoAntecedente" target-entity="CtlTipoAntecedente">
      <join-columns>
        <join-column name="id_tipo_antecedente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
