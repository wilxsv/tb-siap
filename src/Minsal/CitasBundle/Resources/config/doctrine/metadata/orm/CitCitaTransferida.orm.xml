<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CitCitaTransferida" table="cit_cita_transferida">
    <indexes>
      <index name="IDX_E3BBD60B64DB2212" columns="id_cita_antigua"/>
      <index name="IDX_E3BBD60B33C1DC3" columns="id_cita_nueva"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="idCitaNueva" target-entity="CitCitasDia">
      <join-columns>
        <join-column name="id_cita_nueva" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idCitaAntigua" target-entity="CitCitasDia">
      <join-columns>
        <join-column name="id_cita_antigua" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
