<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="LabAreasxestablecimiento" table="lab_areasxestablecimiento">
    <indexes>
      <index name="fki_establecimiento_areasxestablecimiento" columns="idestablecimiento"/>
      <index name="IDX_4583108545BCCC8" columns="idarea"/>
      <index name="IDX_458310856724C8DC" columns="idusuariomod"/>
      <index name="IDX_4583108513B895A1" columns="idusuarioreg"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="condicion" type="string" column="condicion" nullable="false"/>
    <field name="fechahorareg" type="datetime" column="fechahorareg" nullable="true"/>
    <field name="fechahoramod" type="datetime" column="fechahoramod" nullable="true"/>
    <many-to-one field="idusuarioreg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="idusuarioreg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idusuariomod" target-entity="FosUserUser">
      <join-columns>
        <join-column name="idusuariomod" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idestablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="idestablecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idarea" target-entity="CtlAreaServicioDiagnostico">
      <join-columns>
        <join-column name="idarea" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
