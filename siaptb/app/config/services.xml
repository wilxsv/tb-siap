        <service id="sonata.admin.tarsectar" class="Minsal\SeguimientoBundle\Admin\TarSecTarAdmin">
            <tag name="sonata.admin" manager_type="orm" group="AD-1-Administración" label="Ficha Tar" show_in_dashboard="false"/>
