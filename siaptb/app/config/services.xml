        </service>
        <service id="sonata.admin.sechistoriallugar" class="Minsal\SeguimientoBundle\Admin\SecHistorialLugarAdmin">
            <tag name="sonata.admin" manager_type="orm" show_in_dashboard="false" />
            <argument />
            <argument>Minsal\SeguimientoBundle\Entity\SecHistorialLugar</argument>
            <argument>MinsalSeguimientoBundle:SecHistorialLugarAdmin</argument>
        </service>
        <service id="sonata.admin.secmotivoconsulta" class="Minsal\SeguimientoBundle\Admin\SecMotivoConsultaAdmin">
            <tag name="sonata.admin" manager_type="orm" show_in_dashboard="false" />
            <argument />
            <argument>Minsal\SeguimientoBundle\Entity\SecMotivoConsulta</argument>
            <argument>SonataAdminBundle:CRUD</argument>
        </service>
        <!--Test Generacion de Formularios-->
        <service id="sonata.admin.frmformulario" class="Application\CoreBundle\Admin\FrmFormularioAdmin">
            <tag name="sonata.admin" manager_type="orm" group="AD-1-Administración" label="Generacion Formularios"/>
            <argument />
            <argument>Application\CoreBundle\Entity\FrmFormulario</argument>
            <argument>ApplicationCoreBundle:FrmFormularioAdmin</argument>
        </service>
        <!--Tutorial de Usuario-->
        <service id="sonata.admin.mntusertour" class="Application\CoreBundle\Admin\MntUserTourAdmin">
            <tag name="sonata.admin" manager_type="orm" group="AD-1-Administración" label="Tutorial de Usuario" show_in_dashboard="false"/>
            <argument />
            <argument>Application\CoreBundle\Entity\MntUserTour</argument>
            <argument>ApplicationCoreBundle:MntUserTourAdmin</argument>
        </service>
         <!-- SEGUIMIENTO - ANTECEDENTES-->
        <service id="sonata.admin.secantecedentes" class="Minsal\SeguimientoBundle\Admin\SecAntecedentesAdmin">
            <tag name="sonata.admin" manager_type="orm" group="AD-1-Administración" label="Antecedentes" show_in_dashboard="false"/>
            <argument />
            <argument>Minsal\SeguimientoBundle\Entity\SecAntecedentes</argument>
            <argument>MinsalSeguimientoBundle:SecAntecedentesAdmin</argument>
        </service>
        <!-- FIN SEGUIMIENTO - ANTECEDENTES-->
        <!-- SEGUIMIENTO - fvih01-->
        <service id="sonata.admin.tarsolicitudfvih" class="Minsal\SeguimientoBundle\Admin\TarSolicitudFvihAdmin">
            <tag name="sonata.admin" manager_type="orm" group="AD-1-Administración" label="FVIH" show_in_dashboard="false"/>
