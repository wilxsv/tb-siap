{% set _version = 1.0 %}
<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link href="{{ asset('bundles/applicationcore/images/siap_icon.png') }}?v={{ _version }}" type="image/png" rel="Shortcut Icon" />
        <link rel="stylesheet" href="{{ asset('bundles/applicationcore/css/seleccion_modulo.css') }}?v={{ _version }}" type="text/css" media="all" />
        <!-- Latest compiled and minified CSS -->
        <link rel="stylesheet" href="{{ asset('bundles/applicationcore/css/bootstrap3/css/bootstrap.min.css') }}?v={{ _version }}">
