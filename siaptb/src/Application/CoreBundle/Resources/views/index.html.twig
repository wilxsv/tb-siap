        <link rel="stylesheet" href="{{ asset('bundles/applicationcore/css/bootstrap3/css/bootstrap-theme.min.css') }}?v={{ _version }}">
        <script src="{{ asset('bundles/sonatajquery/jquery-1.10.2.js') }}?v={{ _version }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sonatajquery/jquery-ui-1.10.4.js') }}?v={{ _version }}" type="text/javascript"></script>
        <script src="{{ asset('bundles/sonatajquery/jquery-ui-i18n.js') }}?v={{ _version }}" type="text/javascript"></script>
        <!-- Latest compiled and minified JavaScript -->
        <script src="{{ asset('bundles/applicationcore/css/bootstrap3/js/bootstrap.min.js') }}?v={{ _version }}" type="text/javascript"></script>
        <script type="text/javascript">
            jQuery(document).ready(function($) {
                $('.carousel').carousel({
                    interval: 3000
                });
            });
        </script>
        <title>Ministerio de Salud de El Salvador</title>
    </head>
    <body>
        <div id="pagina">
            <div id="pagina-content">
                <div id="content">
