                            <tr><td id="cont-body">
                                    <div class="box-settings shadow">
                                        <div class="row over">
                                            {% if urlModule is defined %}
                                                {% for module in urlModule %}
                                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12{% if hiddeModules|length > 0 and loop.index in hiddeModules %} hidden{% endif %}">
                                                        <a href="{{ module }}">
                                                            <div class="module module{{ loop.index }} pull-right"></div>
                                                        </a>
                                                    </div>
                                                {% endfor %}
                                            {% endif %}
                                        </div>
                                        <!-- <div id="cover-carousel" class="carousel slide" data-ride="carousel"> -->
                                        <div id="cover-carousel">
                                            <!-- Indicators -->
                                            <!-- <ol class="carousel-indicators">
                                                <li data-target="#cover-carousel" data-slide-to="0" class="active"></li>
                                                <li data-target="#cover-carousel" data-slide-to="1"></li>
                                                <li data-target="#cover-carousel" data-slide-to="2"></li>
                                                <li data-target="#cover-carousel" data-slide-to="3"></li>
                                            </ol> -->

                                            <!-- Wrapper for slides -->
                                            <div class="carousel-inner">
                                                <div class="item active">
                                                    <img src="{{ asset('bundles/applicationcore/images/siap_index_img_shadow.png') }}?v={{ _version }}" alt="img01">
                                                </div>
                                                <!-- <div class="item active">
                                                    <img src="{{ asset('bundles/applicationcore/images/cover01.png') }}?v={{ _version }}" alt="img01">
                                                </div> -->
                                                <!-- <div class="item">
                                                    <img src="{{ asset('bundles/applicationcore/images/cover02.png') }}?v={{ _version }}" alt="img02">
                                                </div>
                                                <div class="item">
                                                    <img src="{{ asset('bundles/applicationcore/images/cover03.png') }}?v={{ _version }}" alt="img03">
                                                </div>
                                                <div class="item">
                                                    <img src="{{ asset('bundles/applicationcore/images/cover04.png') }}?v={{ _version }}" alt="img04">
                                                </div> -->
                                            </div>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                            <tr><td id="cont-footer"><div id="footer"></div></td></tr>
                        </table>
                    </center>
                </div>
            </div>
        </div>
        <div class="page-footer">
                <img class="dtic-footer" src="{{ asset("bundles/applicationcore/images/dtic.png") }}?v={{ _version }}" alt="dtic"/>Direcci&oacute;n de Tecnolog&iacute;as de Informaci&oacute;n y Comunicaciones - Ministerio de Salud
            </div>
    </body>
</html>
