<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SecSolicitudQuirurgica" table="sec_solicitud_quirurgica">
    <indexes>
      <index name="IDX_3527F8231827296" columns="id_historial_clinico"/>
      <index name="IDX_3527F82A42231CC" columns="id_manejo_cirugia"/>
      <index name="IDX_3527F825F193BFE" columns="id_prioridad_cirugia"/>
      <index name="IDX_3527F8287B45C92" columns="id_grado_complejidad_quirurgica"/>
      <index name="IDX_3527F82CAE272A7" columns="id_riesgo_anestesico"/>
      <index name="IDX_3527F82A7C48DCA" columns="id_estado_solicitud_quirurgica"/>
      <index name="IDX_3527F82890253C7" columns="id_empleado"/>
      <index name="IDX_3527F82FCF8192D" columns="id_usuario"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="tiempoQuirurgicoEstimado" type="text" column="tiempo_quirurgico_estimado" nullable="true"/>
    <field name="observaciones" type="text" column="observaciones" nullable="true"/>
    <field name="solicitudMateriales" type="text" column="solicitud_materiales" nullable="true"/>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <field name="fechaHoraRegistro" type="datetime" column="fecha_hora_registro" nullable="false"/>
    <many-to-one field="idEstadoSolicitudQuirurgica" target-entity="CtlEstadoSolicitudQuirurgica">
      <join-columns>
        <join-column name="id_estado_solicitud_quirurgica" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsuario" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_usuario" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idRiesgoAnestesico" target-entity="CtlRiesgoAnestesico">
      <join-columns>
        <join-column name="id_riesgo_anestesico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idGradoComplejidadQuirurgica" target-entity="CtlGradoComplejidadQuirurgica">
      <join-columns>
        <join-column name="id_grado_complejidad_quirurgica" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idManejoCirugia" target-entity="TarDatosManejo">
      <join-columns>
        <join-column name="id_manejo_cirugia" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idPrioridadCirugia" target-entity="CtlPrioridadCirugia">
      <join-columns>
        <join-column name="id_prioridad_cirugia" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idHistorialClinico" target-entity="SecHistorialClinico">
      <join-columns>
        <join-column name="id_historial_clinico" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
