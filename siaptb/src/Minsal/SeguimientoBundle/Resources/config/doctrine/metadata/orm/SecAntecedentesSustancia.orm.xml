<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SecAntecedentesSustancia" table="sec_antecedentes_sustancia">
    <indexes>
      <index name="fki_antecedentes_antecedentes_sustancia" columns="id_antecedentes"/>
      <index name="fki_sustancia_antecedentes_sustancia" columns="id_sustancia"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="idSustancia" target-entity="CtlSustancia">
      <join-columns>
        <join-column name="id_sustancia" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAntecedentes" target-entity="SecAntecedentes">
      <join-columns>
        <join-column name="id_antecedentes" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
