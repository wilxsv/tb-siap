	</parameter>
	<parameter name="id_procedimiento_establecimiento" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[2]]></defaultValueExpression>
	</parameter>
	<parameter name="restriccion_especialidad" class="java.lang.String" isForPrompting="false">
		<defaultValueExpression><![CDATA[$P{id_procedimiento_establecimiento} !=0 ? " AND t04.id="+$P{id_procedimiento_establecimiento}:""]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT 	CONCAT_WS(' ',t05.codigo,'-',t05.nombre_comun) as nombre_procedimiento,
	coalesce(initcap(t07.nombreempleado),'No Hay Empleado Asignado') as nombreempleado,
	TO_CHAR(t01.fecha,'DD/MM/YYYY') AS fecha,
	TO_CHAR(t01.fechahorareg,'DD/MM/YYYY') AS fecha_otorgamiento,
	t06.numero as numero_expediente,
	REGEXP_REPLACE(REGEXP_REPLACE(initcap(CONCAT_WS(' ', t08.primer_apellido, t08.segundo_apellido, t08.apellido_casada,',',t08.primer_nombre, t08.segundo_nombre, t08.tercer_nombre)),'( ){2,}', ' ','g'),' ,',',') AS nombre_paciente,
	CONCAT(TO_CHAR(t03.hora_ini, 'HH12:MI:SS AM'),' - ',TO_CHAR(t03.hora_fin, 'HH12:MI:SS AM')) AS rango_hora,
	CASE WHEN t11.id_tipo_empleado in(4) THEN 'Seguimiento Clínico'
	     WHEN t11.id_tipo_empleado in (1,6,7) THEN 'Área de citas'
	     END as modulo,
(select initcap(nombre) from ctl_establecimiento where configurado=true) as nombre
FROM cit_citas_procedimientos t01
	INNER JOIN cit_distribucion_procedimiento t02 ON (t02.id=t01.id_distribucion_procedimiento)
	INNER JOIN mnt_rangohora  t03 ON (t03.id=t02.id_rangohora)
	INNER JOIN mnt_procedimiento_establecimiento t04 ON(t04.id=t02.id_procedimiento_establecimiento $P!{restriccion_especialidad})
	INNER JOIN mnt_ciq t05 ON (t05.id=t04.id_ciq)
	INNER JOIN mnt_expediente t06 ON (t06.id=t01.id_expediente)
	LEFT JOIN mnt_empleado t07 ON (t07.id=t02.id_empleado)
	INNER JOIN mnt_paciente t08 ON (t08.id=t06.id_paciente)
	INNER JOIN fos_user_user t10 ON (t10.id=t01.idusuarioreg)
	LEFT JOIN mnt_empleado t11 ON (t11.id=t10.id_empleado)
WHERE t01.id_estado NOT IN (3,9) AND fecha BETWEEN date($P{fechaInicial}) AND date($P{fechaFinal})
ORDER BY t05.procedimiento ASC, coalesce(initcap(t07.nombreempleado),'No Hay Empleado Asignado') ASC,t01.fecha ASC,t03.hora_ini ASC,t03.hora_fin ASC]]>
	</queryString>
