<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CitJustificacion" table="cit_justificacion">
    <indexes>
      <index name="IDX_BF66A4D442E04C4F" columns="id_estado_cita"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="motivo" length="500" nullable="true"/>
    <many-to-one field="idEstadoCita" target-entity="CitEstadoCita">
      <join-columns>
        <join-column name="id_estado_cita" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
