<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CitTipoDistribucion" table="cit_tipo_distribucion">
    <indexes>
      <index name="IDX_6E46E176D58F2214" columns="id_usuario_registra"/>
      <index name="IDX_6E46E176721098ED" columns="id_usuario_modifica"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="250" nullable="false"/>
    <field name="descripcion" type="text" column="descripcion" nullable="true"/>
    <field name="fechaHoraRegistra" type="datetime" column="fecha_hora_registra" nullable="false"/>
    <field name="fechaHoraModifica" type="datetime" column="fecha_hora_modifica" nullable="true"/>
    <many-to-one field="idUsuarioModifica" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_usuario_modifica" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idUsuarioRegistra" target-entity="FosUserUser">
      <join-columns>
        <join-column name="id_usuario_registra" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
