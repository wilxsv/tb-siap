<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MntDatoReferencia" table="mnt_dato_referencia">
    <indexes>
      <index name="IDX_1883A6AF3EF1ABD3" columns="id_expediente_referido"/>
      <index name="IDX_1883A6AF890253C7" columns="id_empleado"/>
      <index name="IDX_1883A6AF8627A85B" columns="id_aten_area_mod_estab"/>
      <index name="IDX_1883A6AF13B895A1" columns="idusuarioreg"/>
      <index name="IDX_1883A6AF7DFA12F6" columns="id_establecimiento"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fechaHorareg" type="time" column="fecha_horareg" nullable="true"/>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idusuarioreg" target-entity="FosUserUser">
      <join-columns>
        <join-column name="idusuarioreg" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idAtenAreaModEstab" target-entity="MntAtenAreaModEstab">
      <join-columns>
        <join-column name="id_aten_area_mod_estab" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idExpedienteReferido" target-entity="MntExpedienteReferido">
      <join-columns>
        <join-column name="id_expediente_referido" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
